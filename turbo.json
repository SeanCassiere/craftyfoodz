{
  "$schema": "https://turborepo.org/schema.json",
  "globalDependencies": ["**/.env"],
  "pipeline": {
    "db:seed": {
      "inputs": ["seed.ts"],
      "cache": false
    },
    "db:push": {
      "inputs": [],
      "cache": false
    },
    "dev": {
      "persistent": true,
      "cache": false
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": [".next/**", "!.next/cache/**"]
    },
    "lint": {},
    "lint:fix": {},
    "clean": {
      "cache": false
    },
    "//#clean": {
      "cache": false
    },
    "type-check": {
      "dependsOn": [],
      "cache": false
    }
  },
  "globalEnv": [
    "CI",
    "DATABASE_URL",
    "NODE_ENV",
    "SKIP_ENV_VALIDATION",
    "VERCEL",
    "VERCEL_URL",
    "SAP_PUBLIC_URL",
    "SAP_SECRET",
    "SAP_SENDGRID_API_KEY",
    "SAP_SENDGRID_FROM_EMAIL",
    "SAP_SENDGRID_LOGIN_TEMP_ID"
  ]
}
